node_init:   yarn init -y; git init; gibo dump Node > .gitignore; yarn add typescript -D; yarn tsc --init
gem_install: bundle install --path=vendor/bundle --binstubs=vendor/bin
rails_init:  bundle exec rails new . --api --skip-yarn --skip-action-mailer --skip-active-storage --skip-action-cable --skip-sprockets --skip-spring --skip-listen --skip-coffee --skip-javascript --skip-turbolinks --skip-test --skip-system-test
