{
  "suggest.autoTrigger": "always",
  "suggest.minTriggerInputLength": 1,
  "suggest.asciiCharactersOnly": true,
  "suggest.snippetIndicator": " >>",
  "suggest.languageSourcePriority": 200,

  "tabnine.priority": 100,

  "snippets.priority": 300,
  "snippets.loadFromExtensions": false,
  "snippets.textmateSnippetsRoots": ["~/.vsnip"],

  "diagnostic.checkCurrentLine": true,
  "diagnostic.messageDelay": 0,
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",

  "coc.preferences.promptInput": true,
  "coc.preferences.useQuickfixForLocations": true,

  "prettier.disableLanguages": ["markdown"],

  "explorer.sources": [{ "name": "file", "expand": true }],
  "explorer.icon.enableVimDevicons": true,
  "explorer.previewAction.onHover": false,
  "explorer.activeMode": false,
  "explorer.file.autoReveal": false,
  "explorer.keyMappings.global": {
    "i": false,
    "<Tab>": "actionMenu",

    "k": "nodePrev",
    "j": "nodeNext",
    "t": ["expanded?", "collapse", "expand"],
    "<CR>": "open",

    "h": ["expanded?", "collapse", "gotoParent"],
    "l": ["expandable?", "expand", "open"],

    "x": "toggleSelection",

    "y": "copyFilepath",
    "Y": "copyFilename",
    "c": "copyFile",
    "p": "pasteFile",
    "d": "delete",
    "D": "deleteForever",

    "N": "addFile",
    "K": "addDirectory",
    "r": "rename",

    "!": "toggleHidden",
    "R": "refresh",

    "?": "help",
    "q": "quit",
    "<esc>": "esc"
  },

  "languageserver": {
    "efm": {
      "command": "efm-langserver",
      "filetypes": ["go", "ruby", "vim", "sh", "zsh", "markdown"]
    }
  },

  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "deno.unstable": true,
  "deno.codeLens.implementations": true,
  "deno.codeLens.references": true,
  "deno.codeLens.referencesAllFunctions": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.onlyUseLocalVersion": true,
  "prettier.statusItemText": "",

  "rust-analyzer.cargo.loadOutDirsFromCheck": true,
  "rust-analyzer.procMacro.enable": true,

  "python.jediEnabled": false,
  "python.linting.flake8Enabled": true,

  "json.schemas": [
    {
      "url": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/api/json/catalog.json",
      "fileMatch": ["/*"]
    }
  ],
  "yaml.schemas": {
    "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/api/json/catalog.json": "/*",
    "https://raw.githubusercontent.com/docker/cli/master/cli/compose/schema/data/config_schema_v3.9.json": [
      "/docker-compose.yml"
    ]
  },

  "cSpell.showStatus": false,
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.enabledLanguageIds": [
    "vim",
    "css",
    "git-commit",
    "go",
    "html",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "markdown",
    "python",
    "ruby",
    "typescript",
    "typescript.tsx",
    "typescriptreact",
    "yaml"
  ],
  "tsserver.enable": false,
  "deno.enable": true,
  "deno.lint": true,
  "coc.source.file.trimSameExts": [],
  "prettier.disableLanguage": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ]
}
