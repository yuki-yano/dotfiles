{
  "hover.floatConfig": {
    "border": true,
    "highlight": "CocHoverFloating"
  },
  "suggest.autoTrigger": "always",
  "suggest.minTriggerInputLength": 1,
  "suggest.asciiCharactersOnly": true,
  "suggest.snippetIndicator": " >>",
  "suggest.languageSourcePriority": 200,
  "suggest.floatConfig": {
    "border": true,
    "highlight": "CocSuggestFloating"
  },
  "signature.floatConfig": {
    "border": true,
    "highlight": "CocSignatureFloating"
  },
  "coc.source.ultisnips-select.priority": 500,
  "tabnine.priority": 100,
  "diagnostic.messageTarget": "float",
  "diagnostic.checkCurrentLine": true,
  "diagnostic.messageDelay": 0,
  "diagnostic.errorSign": "",
  "diagnostic.warningSign": "",
  "diagnostic.infoSign": "",
  "diagnostic.hintSign": "",
  "diagnostic.floatConfig": {
    "border": true,
    "highlight": "CocDiagnosticFloating"
  },
  "coc.preferences.promptInput": true,
  "coc.preferences.useQuickfixForLocations": false,
  "diagnostic.locationlistUpdate": false,
  "prettier.disableLanguages": [
    "markdown"
  ],
  "explorer.sources": [
    {
      "name": "file",
      "expand": true
    }
  ],
  "explorer.previewAction.onHover": false,
  "explorer.keyMappings.global": {
    "i": false,
    "<Tab>": "actionMenu",
    "k": "nodePrev",
    "j": "nodeNext",
    "t": [
      "expanded?",
      "collapse",
      "expand"
    ],
    "<CR>": "open",
    "h": [
      "expanded?",
      "collapse",
      "gotoParent"
    ],
    "l": [
      "expandable?",
      "expand",
      "open"
    ],
    "x": "toggleSelection",
    "y": "copyFilepath",
    "Y": "copyFilename",
    "c": "copyFile",
    "p": "pasteFile",
    "d": "delete",
    "D": "deleteForever",
    "N": "addFile",
    "K": "addDirectory",
    "r": "rename",
    "!": "toggleHidden",
    "R": "refresh",
    "?": "help",
    "q": "quit",
    "<esc>": "esc"
  },
  "languageserver": {
    "efm": {
      "command": "efm-langserver",
      "filetypes": [
        "go",
        "ruby",
        "vim",
        "sh",
        "zsh",
        "markdown"
      ]
    }
  },
  "typescript.preferences.importModuleSpecifier": "relative",
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
  "deno.unstable": true,
  "deno.codeLens.implementations": true,
  "deno.codeLens.references": true,
  "deno.codeLens.referencesAllFunctions": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact"
  ],
  "prettier.onlyUseLocalVersion": true,
  "prettier.statusItemText": "",
  "rust-analyzer.procMacro.enable": true,
  "python.jediEnabled": false,
  "python.linting.flake8Enabled": true,
  "solargraph.diagnostics": true,
  "json.schemas": [
    {
      "url": "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/api/json/catalog.json",
      "fileMatch": [
        "/*"
      ]
    }
  ],
  "yaml.schemas": {
    "https://raw.githubusercontent.com/SchemaStore/schemastore/master/src/api/json/catalog.json": "/*",
    "https://raw.githubusercontent.com/docker/cli/master/cli/compose/schema/data/config_schema_v3.9.json": [
      "/docker-compose.yml"
    ]
  },
  "prisma.prismaFmtBinPath": "",
  "cSpell.showStatus": false,
  "cSpell.diagnosticLevel": "Hint",
  "cSpell.enabledLanguageIds": [
    "vim",
    "css",
    "git-commit",
    "go",
    "html",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "markdown",
    "python",
    "ruby",
    "typescript",
    "typescript.tsx",
    "typescriptreact",
    "yaml"
  ],
  "tsserver.enable": false,
  "deno.enable": false,
  "deno.lint": true,
  "coc.source.file.trimSameExts": []
}
